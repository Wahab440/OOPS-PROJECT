<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.librarymanagement.Member.BorrowingHistoryController">
    <Label alignment="CENTER" prefHeight="50.0" prefWidth="400.0" style="-fx-background-color: #011a11;" text="BORROWING HISTORY" textFill="WHITE">
        <font>
          <Font name="System Bold Italic" size="28.0" />
        </font>
    </Label>
    <Button fx:id="Backbtn" layoutX="160.0" layoutY="346.0" mnemonicParsing="false" onAction="#GoBack" prefHeight="26.0" prefWidth="80.0" style="-fx-background-color: #011a11;" text="BACK" textFill="WHITE">
        <font>
          <Font name="System Bold" size="14.0" />
        </font>
    </Button>
    <TableView fx:id="BorrowedTable" layoutX="14.0" layoutY="66.0" prefHeight="260.0" prefWidth="376.0">
      <columns>
          <TableColumn fx:id="ISBNCol" prefWidth="128.0" text="ISBN" />
          <TableColumn fx:id="IssueDateCol" prefWidth="133.0" text="IssueDate" />
          <TableColumn fx:id="ReturnDateCol" prefWidth="114.0" text="ReturnDate" />
      </columns>
    </TableView>
</AnchorPane>
