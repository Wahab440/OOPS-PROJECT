<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: darkgrey;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.librarymanagementapp.MemberLoginController">
    <Label alignment="CENTER" layoutX="-1.0" prefHeight="62.0" prefWidth="606.0" style="-fx-background-color: #011a11;" text="MEMBER LOGIN" textFill="WHITE">
        <font>
          <Font name="Berlin Sans FB Bold" size="24.0" />
        </font>
    </Label>
    <Label layoutX="215.0" layoutY="110.0" text="USERNAME">
        <font>
          <Font name="Berlin Sans FB Bold" size="18.0" />
        </font>
    </Label>
    <Label layoutX="216.0" layoutY="185.0" text="PASSWORD">
        <font>
          <Font name="Berlin Sans FB Bold" size="18.0" />
        </font>
    </Label>
    <TextField fx:id="UserNametf" layoutX="212.0" layoutY="138.0" prefHeight="25.0" prefWidth="200.0" style="-fx-background-radius: 15;" />
    <Button fx:id="Loginbtn" layoutX="264.0" layoutY="278.0" mnemonicParsing="false" onAction="#LoginUser" style="-fx-background-color: #011a11; -fx-background-radius: 15;" text="LOGIN" textFill="WHITE">
        <font>
          <Font name="Berlin Sans FB Bold" size="16.0" />
        </font>
    </Button>
    <Label layoutX="215.0" layoutY="240.0" prefHeight="35.0" prefWidth="124.0" text="Don't have an account?" />
    <Hyperlink fx:id="RegisterLink" layoutX="339.0" layoutY="246.0" onAction="#OpenRegisterPage" prefHeight="0.0" prefWidth="51.0" text="Register" />
    <PasswordField fx:id="Passwordpf" layoutX="212.0" layoutY="210.0" prefHeight="25.0" prefWidth="200.0" style="-fx-background-radius: 15;" />
</AnchorPane>
